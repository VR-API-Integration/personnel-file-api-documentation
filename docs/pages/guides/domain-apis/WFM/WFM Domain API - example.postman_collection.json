	"item": [
		{
			"name": "Get access token",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = JSON.parse(responseBody)\r",
							"\r",
							"postman.setGlobalVariable(\"AccessToken\", jsonData.access_token);"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "urlencoded",
					"urlencoded": [
						{
							"key": "client_id",
							"value": "{{VCClientId}}",
							"type": "text"
						},
						{
							"key": "client_secret",
							"value": "{{VCClientSecret}}",
							"type": "text"
						},
						{
							"key": "grant_type",
							"value": "client_credentials",
							"type": "text"
						},
						{
							"key": "tenant_id",
							"value": "{{VCTenantId}}",
							"type": "text"
						}
					]
				},
				"url": {
					"raw": "https://connect.visma.com/connect/token",
					"protocol": "https",
					"host": [
						"connect",
						"visma",
						"com"
					],
					"path": [
						"connect",
						"token"
					]
				}
			},
			"response": []
		},
